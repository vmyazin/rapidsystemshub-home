{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": "astro",
    "buildCommand": "pnpm build",
    "outputDirectory": ".vercel/output",
    "headers": [{
        "source": "/(.*)\\.(js|css|woff2|png|jpg|jpeg|webp|svg|ico)",
        "headers": [{
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
        }]
    }]
}